{
  "name": "@effectful/debugger",
  "version": "1.4.8",
  "description": "User space debugging",
  "main": "vscode.js",
  "repository": "https://github.com/awto/effectfuljs.git",
  "author": "Vitalii Akimov <vitaliy.akimov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tslint --project . && jest",
    "build:transform": "babel src/transform.js --out-file transform.js",
    "build:decls": "tsc",
    "build": "npm run build:transform && npm run build:decls",
    "build:rt": "babel --extensions \".ts,.js\" --out-dir . src",
    "build:deps": "node ./buildDeps",
    "build-loose": "npm run build:rt && npm run build:deps",
    "link-test": "node ./test/makeLinks",
    "docs": "rimraf api && typedoc --plugin typedoc-plugin-markdown --out api --excludePrivate src --readme none"
  },
  "browser": {
    "worker_threads": false,
    "ws": false,
    "vm": false
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@types/asap": "^2.0.0",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.5.7",
    "assert": "^2.0.0",
    "babel-loader": "^9.1.3",
    "core-js-pure": "^3.16.1",
    "events": "^3.3.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jsdom": "^22.1.0",
    "lodash.sortby": "^4.7.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.7.2",
    "rimraf": "^5.0.1",
    "tslint": "^6.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.50.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/generator": "^7.4.4",
    "@babel/parser": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-numeric-separator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-function-sent": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.0",
    "@babel/plugin-transform-destructuring": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/plugin-transform-parameters": "^7.7.7",
    "@babel/plugin-transform-property-mutators": "^7.10.1",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-shorthand-properties": "^7.14.5",
    "@babel/plugin-transform-spread": "^7.6.2",
    "@babel/plugin-transform-template-literals": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@effectful/core": "^1.4.7",
    "@effectful/serialization": "^1.4.7",
    "@vscode/debugprotocol": "^1.35.0",
    "asap": "^2.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "foreground-child": "^3.1.1",
    "lodash": "^4.17.21",
    "make-dir": "^4.0.0",
    "minimatch": "^9.0.3",
    "promise": "^8.0.3",
    "spawn-wrap": "^2.0.0",
    "ws": "^8.0.0"
  }
}
